LR_IROM1 0x200000000 0x00008000 {
        ER_IROM1 0x200000000 0x00008000 {
                *.o (RESET, +First)
                *.(InRoot$$Sections)
                .ANY (+RO)
        }
        RW_IRAM1 0x200000000 0x00008000 {
                .ANY (+RW +ZI)
        }
}
