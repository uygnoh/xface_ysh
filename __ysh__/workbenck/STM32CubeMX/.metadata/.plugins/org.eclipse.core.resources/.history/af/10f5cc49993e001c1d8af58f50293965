#include "bsp_uart.h"

//UART__发送完成（中断）回调函数
void HAL_UART_TxCpltCallback(UART_HandleTypeDef *huart1)
{
        uart_ready = SET;      //设置传输完成标志位（SET传输完成）
}
//UART__接收完成（中断）回调函数
void HAL_UART_RxCpltCallback(UART_HandleTypeDef *huart1)
{
        uart_ready = SET;      //设置传输完成标志位（SET传输完成）
}
//UART__出错（中断）回调函数
void HAL_UART_ErrorCallback(UART_HandleTypeDef *huart1)
{

}
